{
  "name": "home-hub",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "2.1.3",
    "lodash": "^4.17.11",
    "redux-thunk": "^2.3.0",
    "superagent": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rm -rf build",
    "build:react": "react-scripts build",
    "build": "npm run clean && ./build.sh",
    "test": "react-scripts test",
    "test:coverage": "CI=true react-scripts test --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/index.js --collectCoverageFrom=!src/config.js",
    "eject": "react-scripts eject",
    "dev": "PORT=3010 react-scripts start",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
